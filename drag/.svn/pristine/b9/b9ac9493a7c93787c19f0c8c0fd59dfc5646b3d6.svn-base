body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td, hr, button, article, aside, details, figcaption, figure, footer, header, hgroup, menu, nav, section {
    margin: 0;
    padding: 0
}

input:-webkit-autofill, textarea:-webkit-autofill, select:-webkit-autofill, input:disabled, textarea:disabled, select:disabled, input, textarea, select {
    background: none !important;
}

body, button, input, select, textarea {
    font: 14px/1.5 sans-serif, arial, Tahoma, Verdana, STHeiTi, simsun;
    color: #333;
    outline: none;
    border: none;
}

ul, li, ol {
    list-style: none;
}

i, cite, em, var, address, dfn {
    font-style: normal;
}

img {
    border: 0 none;
}

input, select {
    vertical-align: middle;
}

table {
    border-collapse: collapse;
}

s, em, i {
    font-style: normal;
    text-decoration: none;
}

a {
    outline: none;
    text-decoration: none;
}

body, html {
    width: 100%;
    height: 100%;
    min-width: 1200px;
    overflow-y: hidden;
}

.drag-container {
    width: 100%;
    height: 100%;
    overflow: auto;
    overflow-y: hidden;
    min-width: 1200px;
    position: relative
}

.drag-btn-sub {
    position: absolute;
    right: 20px;
    top: 10px;
    color: #fff;
    background-color: #2d8cf0;
    border-color: #2d8cf0;
    padding: 10px 25px;
    border-radius: 5px;
    cursor: pointer;
    font-size: 14px;
    z-index: 1000
}

.drag-page {
    height: 60px;
    width: 100%;
    min-width: 1200px;
    border-bottom: 1px solid #ddd;
    position: fixed;
    top: 0;
    text-align: center;
    box-sizing: border-box
}

.drag-page ul {
    width: calc(100% - 260px - 180px);
    margin-left: 180px;
    text-align: center;
    height: 100%;
    vertical-align: middle;
    line-height: 52px
}

.drag-page a {
    padding: 6px;
    margin: 5px;
    font-size: 13px;
    border: 1px solid #ccc;
    color: #585858;
    display: inline-block;
    vertical-align: middle;
    line-height: 12px;
    text-decoration: none;
    box-sizing: border-box;
    border-radius: 3px;
    cursor: pointer;
}

.drag-page .lmw-current {
    background: #2d8cf0;
    color: white;
    border: 1px solid #2d8cf0;
    box-sizing: border-box
}

.drag-contentbox {
    position: absolute;
    height: calc(100% - 60px);
    margin-top: 60px;
    width: 100%;
    min-width: 1200px;
    zoom: 1
}

.drag-contentbox:after {
    visibility: hidden;
    display: block;
    font-size: 0;
    content: " ";
    clear: both;
    height: 0
}

.drag-left-side,
.drag-right-side,
.drag-center-box {
    float: left;
    width: 15%;
    height: 100%;
    padding: 25px 0;
    box-sizing: border-box;
    overflow-y: auto;
    -moz-appearance: none !important;
    scrollbar-arrow-color: #f7f7f7; /**//*三角箭头的颜色*/
    scrollbar-face-color: #d7d8df; /**//*立体滚动条的颜色*/
    scrollbar-highlight-color: #666; /**//*滚动条空白部分的颜色*/
    scrollbar-shadow-color: #d7d8df; /**//*立体滚动条阴影的颜色*/
    scrollbar-track-color: #f7f7f7; /**//*立体滚动条背景颜色*/
}

.drag-right-side {
    width: 25%;
    float: right;
    border-left: 1px solid #eee;
    box-sizing: border-box;

}

.drag-center-box {
    width: 60%;
    background: #f7f7f7;
    padding: 0;
}

.drag-left-side::-moz-scrollbar,
.drag-right-side::-moz-scrollbar,
.drag-center-box::-moz-scrollbar {
    position: absolute;
    right: -10%;
    width: 4px;
    background-color: #f7f7f7
}

.drag-left-side::-moz-scrollbar-track,
.drag-right-side::-moz-scrollbar-track,
.drag-center-box::-moz-scrollbar-track {
    background: #f7f7f7
}

.drag-left-side::-moz-scrollbar-thumb,
.drag-right-side::-moz-scrollbar-thumb,
.drag-center-box::-moz-scrollbar-thumb {
    border-radius: 18px;
    background: #d7d8df
}

.drag-left-side::-moz-scrollbar-thumb:window-inactive,
.drag-right-side::-moz-scrollbar-thumb:window-inactive,
.drag-center-box::-moz-scrollbar-thumb:window-inactive {
    background: #f7f7f7
}

.drag-left-side::-webkit-scrollbar,
.drag-right-side::-webkit-scrollbar,
.drag-center-box::-webkit-scrollbar {
    position: absolute;
    right: -10%;
    width: 4px;
    background-color: #f7f7f7
}

.drag-left-side::-webkit-scrollbar-track,
.drag-right-side::-webkit-scrollbar-track,
.drag-center-box::-webkit-scrollbar-track {
    background: #f7f7f7
}

.drag-left-side::-webkit-scrollbar-thumb,
.drag-right-side::-webkit-scrollbar-thumb,
.drag-center-box::-webkit-scrollbar-thumb {
    border-radius: 18px;
    background: #d7d8df
}

.drag-left-side::-webkit-scrollbar-thumb:window-inactive,
.drag-right-side::-webkit-scrollbar-thumb:window-inactive,
.drag-center-box::-webkit-scrollbar-thumb:window-inactive {
    background: #f7f7f7
}

.drag-left-side::-o-scrollbar,
.drag-right-side::-o-scrollbar,
.drag-center-box::-o-scrollbar {
    position: absolute;
    right: -10%;
    width: 4px;
    background-color: #f7f7f7
}

.drag-left-side::-o-scrollbar-track,
.drag-right-side::-o-scrollbar-track,
.drag-center-box::-o-scrollbar-track {
    background: #f7f7f7
}

.drag-left-side::-o-scrollbar-thumb,
.drag-right-side::-o-scrollbar-thumb,
.drag-center-box::-o-scrollbar-thumb {
    border-radius: 18px;
    background: #d7d8df
}

.drag-left-side::-o--scrollbar-thumb:window-inactive,
.drag-right-side::-o-scrollbar-thumb:window-inactive,
.drag-center-box::-o-scrollbar-thumb:window-inactive {
    background: #f7f7f7
}

.drag-pdful > li {
    width: 592px;
    height: 842px;
    margin: 20px auto 0;
    position: relative;
    background: #fff
}

.drag-pdful {
    width: 592px;
    margin: 0 auto;
    position: relative
}

.dragStatus,
.dragBox {
    display: block;
    position: absolute;
    width: 130px;
    height: 130px;
    border: 1px dashed #e60012;
    cursor: move;
    z-index: 999;
    text-align: center;
    line-height: 130px
}

.dragStatus img,
.dragBox img {
    display: inline-block;
    max-width: 120px;
    max-height: 120px
}

.dragStatus:hover {
    background: rgba(230, 10, 0, 0.1)
}

.dragStatus:hover .drag-close {
    display: block
}

.dragStatus .drag-close {
    position: absolute;
    top: -13px;
    right: -12px;
    width: 26px;
    height: 26px;
    background: url("../images/ico-drag-close.png") no-repeat center;
    cursor: pointer
}

.drag-goto {
    position: absolute;
    z-index: 300;
    right: 27%;
    bottom: 30px;
    height: 120px;
    width: 50px
}

.drag-goto .goto-bottom,
.drag-goto .goto-top {
    background-position: 50%;
    background-size: contain;
    width: 50px;
    height: 50px;
    position: absolute;
    cursor: pointer
}

.drag-goto .goto-top {
    top: 0;
    background: url("../images/ico-drag-top.png") no-repeat center
}

.drag-goto .goto-bottom {
    bottom: 0;
    background: url("../images/ico-drag-btm.png") no-repeat center
}

.drag-container img {
    max-width: 100%;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

