*{
    padding: 0;
    margin:0;
}
.container{
    width: 100%;
    height: 100%;
    overflow: auto;
    overflow-y: hidden;
    min-width: 1200px;
    position: relative;
}
body,html{
    width: 100%;
    height: 100%;
    min-width: 1200px;
}
li{
    list-style: none;
    text-align: center;
}
.contentbox{
    position: absolute;
    height: calc(100% - 60px);
    margin-top:60px;
    width: 100%;
    overflow: hidden;

}
.wrapper,.right_con{
    width: 180px;
    height: 100%;
    overflow-y: auto;
    float: left;
    padding: 25px 0;
    border-right: 1px solid #dddddd;
    box-sizing: border-box;

}
.right_con{
    width:260px;
    float: right;
    border-left: 1px solid #dddddd;
    box-sizing: border-box;
}
.box {
    width: 130px;
    height: 130px;
    cursor: move;
    text-align: center;
    margin:0 auto 25px;
}
.box>div{
    width: 100px;
    height: 100px;
    margin:0 auto 10px;
    position: relative;

}

.box img{
    max-width: 100px;
    max-height: 100px;
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
}
.dragFinish{
    position: absolute;
    top: 50% !important;
    left: 50% !important;
    -webkit-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);
}
#centerUl{
    /*border: 1px solid black;*/
    width: calc(100% - 260px - 180px);
    height: 100%;
    left: 120px;
    overflow-y: auto;
    background: #eee;
    float: left;
    box-sizing: border-box;

}
#pdfUl> li{
    width: 592px;
    height: 842px;
    margin:10px auto 0px;
    position: relative;
    background: #fff;
}
#pdfUl{
    width:592px;margin:0 auto;
    position: relative;
    padding-bottom: 40px;
}
#temp{
    opacity: 0.3;
    position: absolute;
    width: 120px;

}
.dragStatus{
    /*padding: 10px;*/
    position: absolute;
    font-size: 0;
    width: 130px;
    height: 130px;
}
.dragStatus img{
    width: 120px;
}
.dragStatus:hover{
    background:rgba(230, 10, 0,0.3);
}
.dragStatus:hover .close{
    display: block;
}
.close{
    background: url("./images/close.png") no-repeat center;
    position: absolute;
    top: -13px;
    right: -12px;
    width: 26px;
    height: 26px;
    cursor: pointer;
    display: none;
}
.pagebox{
    height: 60px;
    width: 100%;
    min-width: 1200px;
    border-bottom:1px solid #dddddd;
    position: fixed;
    top:0;
    text-align: center;
    box-sizing: border-box;
}
#ul{
    width:  calc(100% - 260px - 180px);;
    margin-left: 180px;
    text-align: center;
    height: 100%;
    vertical-align: middle;
    line-height: 52px;
}
#pagebox a{
    padding:6px;font-size:13px;border:1px solid #ccc;cursor:pointer;color:#585858;
    display: inline-block;
    vertical-align: middle;
    line-height: 12px;
    text-decoration: none;
    margin:5px;
    box-sizing: border-box;
    border-radius: 3px;
}
#pagebox .lmw-current{background:#2d8cf0;color:white;border:1px solid #2d8cf0;box-sizing: border-box;}
.el-message--error{
    position: fixed;
    right: 200px;
    top:30px;
    color: red;
}
.btn{
    position: absolute;
    right: 20px;
    top: 10px;
    color: #fff;
    background-color: #2d8cf0;
    border-color: #2d8cf0;
    padding: 10px 25px;
    border-radius: 5px;
    cursor: pointer;
    font-size: 14px;
    z-index: 1000;


}
.goto {
    position: absolute;
    z-index: 300;
    right: 280px;
    bottom: 30px;
    height: 120px;
    width: 50px;
}
.goto .goto-bottom, .goto .goto-top {
    background-position: 50%;
    background-size: contain;
    width: 50px;
    height: 50px;
    position: absolute;
    cursor: pointer;
}
.goto-top{
    top:0;
    background: url("./images/row1.png") no-repeat center;


}
.goto-bottom{
    bottom: 0;
    background: url("./images/row2.png") no-repeat center;

}

.wrapper::-webkit-scrollbar{ position: absolute; right: -10%; width: 4px; background-color: #fff; }
.wrapper::-webkit-scrollbar-track{background:#fff;}
.wrapper::-webkit-scrollbar-thumb{border-radius: 18px;background: #D7D8DF;}
.wrapper::-webkit-scrollbar-thumb:window-inactive{background: #fff;}
